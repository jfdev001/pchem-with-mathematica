   RowBox[{
   "Print", "[", 
    "\"\<(1) Find normaliztion constant A of \[Phi](r,\[Alpha]) = \
\!\(\*SuperscriptBox[\(Ae\), \(-\*SuperscriptBox[\(\[Alpha]r\), \(2\)]\)]\) \
using procedures\nin previous steps (i.e. Denominator is < \[Phi] | \[Phi] > \
=\!\(\*SubsuperscriptBox[\(\[Integral]\), \(-\[Infinity]\), \
\(\[Infinity]\)]\)|\[Phi]\!\(\*SuperscriptBox[\(|\), \
\(2\)]\)\!\(\*SuperscriptBox[\(r\), \(2\)]\)dr = 1\>\"", "]"}], "\n", "\n", 
   RowBox[{"(*", 
    RowBox[{"Normalize", " ", "the", " ", "Gaussian"}], "*)"}], "\n", 
   RowBox[{
    RowBox[{"gaussian", " ", "=", " ", 
     RowBox[{"A", "*", 
      RowBox[{"Exp", "[", 
       RowBox[{
        RowBox[{"-", "1"}], "*", "\[Alpha]", "*", 
        RowBox[{"r", "^", "2"}]}], "]"}]}]}], ";", " ", 
    RowBox[{"(*", 
     RowBox[{"Gaussian", " ", "trial", " ", "function"}], "*)"}], "\n", 
    RowBox[{"gaussianNormalizationIntegral", " ", "=", " ", 
     RowBox[{"Normal", "[", 
      RowBox[{"Integrate", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"r", "^", "2"}], "*", 
         RowBox[{"gaussian", "^", "2"}]}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"r", ",", " ", "0", ",", " ", "Infinity"}], "}"}]}], "]"}], 
      "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"gaussianNormalizationConst", " ", "=", " ", 
     RowBox[{"Simplify", "[", 
      RowBox[{"A", "/.", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"Solve", "[", 
          RowBox[{
           RowBox[{
           "gaussianNormalizationIntegral", " ", "\[Equal]", " ", "1"}], ",", 
           " ", "A"}], "]"}], ")"}], "[", 
        RowBox[{"[", "2", "]"}], "]"}]}], "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"normalizedGaussian", " ", "=", " ", 
     RowBox[{"gaussian", "/.", 
      RowBox[{"A", "\[Rule]", "gaussianNormalizationConst"}]}]}], ";"}], "\n", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{"/", "Normalize"}], " ", "the", " ", "Gaussian"}], "*)"}], "\n", 
   "\n", 
